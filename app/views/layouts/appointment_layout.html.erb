<!DOCTYPE html>
<html>
  <head>
    <title>ChoresApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lato&family=Yellowtail&display=swap" rel="stylesheet">
  </head>

  <body>
    <%= render 'layouts/appointment_navbar' %>
    <div class="container-md bg-light bg-gradient appointment-main">
        <%= yield %>
    <%= render 'layouts/footer' %>
    </div>
  </body>

</html>